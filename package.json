{
  "name": "specter-one",
  "hompage": "https://specterone.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "amazon-cognito-auth-js": "^1.1.1",
    "amazon-cognito-identity-js": "^1.28.0",
    "antd": "^2.11.2",
    "aws-sdk": "^2.169.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-facebook-login": "^3.6.2",
    "react-google-login": "^2.9.2",
    "react-parallax": "^1.4.4",
    "react-router-dom": "^4.1.1",
    "scroll-to-element": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.2.1",
    "react-mailchimp-subscribe": "^1.0.1",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "css": "lessc --clean-css src/style.less src/style.min.css",
    "start": "lessc --clean-css src/style.less src/style.min.css && react-scripts start",
